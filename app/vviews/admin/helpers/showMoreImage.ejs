<%
  showMoreImage = (id, array, linkMoreImage) => {
    let html=''
    array.forEach(value=>{
        let name = value.split(".")[0]
        html += `
        <div id="img-${name}" data-name="${value}" class="image-area">
          <img style="width: 100%" src="${linkMoreImage}/${value}" 
          alt="moreImage" 
          onerror="this.onerror=null;this.src='${linkMoreImage}/no-avatar.jpg'">
          <a class="remove-image" href="javascript:deletePhoto('${name}');" style="display: inline;">&#215;</a>
        </div>
        `
    })
    return html
  }
%>